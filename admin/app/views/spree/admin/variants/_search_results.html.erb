<div id="variants_search_results" class="overflow-y-auto overflow-x-hidden" data-search-picker-target="results" style="max-height: 300px">
  <% if variants.any? %>
    <%= render collection: variants, partial: 'spree/admin/variants/search_result', as: :variant %>
  <% else %>
    <p class="text-muted text-center pb-3 pt-5">
      <%= Spree.t('admin.variants.search_results.no_variants') %>
    </p>
  <% end %>
</div>
